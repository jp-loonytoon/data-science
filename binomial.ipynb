   "source": [
    "import math\n",
    "\n",
    "def choose(n, k):\n",
    "    return (math.factorial(n) / math.factorial(k) * math.factorial(n-k))\n",
    "    \n",
    "def binomial(k, n, p):\n",
    "    b = choose(n, k) * p\n",
    "    return b\n",
    "\n"
   ]
